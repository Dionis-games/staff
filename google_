#для программы нужен https://github.com/MarioVilas/googlesearch
#чтобы его установить нужно pip3 install google
f=$(google $@ --stop=10)
declare -i i=0;
for s in $f; 
do
echo $i $s
i=i+1
done
for num in {1..10};
do
read -i ": " choice
i=0
for h in $f; 
do
if [ $((choice - i)) == 0 ];
then 
browser $h
fi
i=i+1
done
done
